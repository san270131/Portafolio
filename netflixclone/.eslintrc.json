{
    "env":{
        "browser": true,
        "es2021": true
    },
    "extends":[
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
       "semi": ["erro", "always"],
       "quotes": ["erro", "double"],
       "@typescript-eslint/no-unused-vars": "error",
       "no-unused-vars": "off"
    }
}